//Collections =>QUEUE (FIFO)
import java.util.*;

public class Main
{
	public static void main(String[] args) {
	    Queue<Integer> q=new LinkedList<>(); //Quueue is an interface so we cannot create an object for it so we should use LinkedList.
	    q.add(100);
	    q.add(200);
	    q.add(300);
	    q.add(400);
	    System.out.println("Size of Elements Is: "+q.size());
		System.out.println("Head Element Is: "+q.peek());
		System.out.println("Removed Element Is: "+q.remove());
		System.out.println("After removing Head Element Is: "+q.peek());
		System.out.println("After removing size of Element Is: "+q.size());
		
	}
}
